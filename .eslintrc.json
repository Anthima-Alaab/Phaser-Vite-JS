{
  "root": true,
  "env": {
    "es6": true,
    "node": false,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:jsdoc/recommended"],
  "plugins": ["jsdoc"],
  "rules": {
    "no-console": "error",
    "jsdoc/no-defaults": "off",
    "jsdoc/require-param-description": "off",
    "jsdoc/require-property-description": "off",
    "jsdoc/require-returns-description": "off"
  }
}
